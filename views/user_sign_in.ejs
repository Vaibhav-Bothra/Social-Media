<html>
  <head>
    <title>SignIn</title>
  </head>
  <body>
    <h1>SIGN IN</h1>
    <form action="/users/create-session" method="post">
      <input type="email" name="email" placeholder="Enter E-mail" required />
      <input
        type="password"
        name="password"
        placeholder="Enter Password"
        required
      />
      <input type="submit" value="Sign In" />
    </form>
  </body>
</html>
